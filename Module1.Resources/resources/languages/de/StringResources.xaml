<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Module location-->
    <system:String x:Key="ModuleLocationFile">Datei</system:String>

    <!-- Command names -->
    <system:String x:Key="OpenCommandName">Aufmachen</system:String>
    <system:String x:Key="CloseCommandName">Schließen</system:String>

    <!-- Other strings -->
    <system:String x:Key="Hello">Guten tag</system:String>


</ResourceDictionary>