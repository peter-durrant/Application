<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Module location-->
    <system:String x:Key="ModuleLocationFile">Fichier</system:String>

    <!-- Command names -->
    <system:String x:Key="PrintCommandName">Impression</system:String>
    <system:String x:Key="ExitCommandName">Sortie</system:String>

    <!-- Other strings -->
    <system:String x:Key="Hello">Salut</system:String>

</ResourceDictionary>